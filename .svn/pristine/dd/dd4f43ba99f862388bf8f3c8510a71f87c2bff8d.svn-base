	$(".inner").load("jsp/actplan.jsp?planId="+planId)
}
function showplan1(planId){
	$(".inner").load("jsp/actplan_talk.jsp?planId="+planId)
}
function showopen_shiju(planId){
	$(".inner").load("jsp/middlecity_push.jsp?planId="+planId)
}
